        <script>
            var toggler = document.getElementsByClassName("caret");
            for (var i = 0; i < toggler.length; i++) {
              toggler[i].addEventListener("click", function() {
                this.parentElement.querySelector(".nested").classList.toggle("active");
                this.classList.toggle("caret-down");
              });
            }
            window.onhashchange = function() {
                if (window.location.hash.includes("Getting-Started")) {
                    document.getElementById("Getting-Started").style.display = "block";
                    document.getElementById("Blocks").style.display = "none";
                    document.getElementById("Extentions").style.display = "none";
                }else if (window.location.hash.includes("Blocks")) {
                    document.getElementById("Getting-Started").style.display = "none";
                    document.getElementById("Blocks").style.display = "block";
                    document.getElementById("Extentions").style.display = "none";
                }else if (window.location.hash.includes("Extentions")) {
                    document.getElementById("Getting-Started").style.display = "none";
                    document.getElementById("Blocks").style.display = "none";
                    document.getElementById("Extentions").style.display = "block";
                }
            };
            window.onhashchange();
